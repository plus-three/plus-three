{
  "name": "plus-three",
  "version": "0.0.2",
  "description": "plus-three",
  "license": "MIT",
  "author": "xiaofei",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch --no-warnings ./scripts/build/index.ts",
    "build": "cross-env NODE_ENV=production npm run clean && tsx --no-warnings ./scripts/build/index.ts",
    "gitcz": "git-cz",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "release": "tsx --no-warnings ./scripts/release.ts",
    "publish": "sh ./scripts/publish.sh",
    "commit": "sh ./scripts/commit.sh",
    "clean": "rimraf dist",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n changelog.config.js",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^23.1.2",
    "cannon-es": "^0.20.0",
    "csstype": "^3.1.3",
    "three": "^0.165.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@plus-three/types": "workspace:*",
    "@plus-three/utils": "workspace:*",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-replace": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chalk": "4.1.2",
    "commitizen": "^4.0.3",
    "consola": "^3.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "cz-customizable": "^6.2.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.31.0",
    "execa": "5.1.0",
    "husky": "^3.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.21.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner2": "^1.2.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "semver": "^7.3.7",
    "string": "^3.3.3",
    "ts-morph": "^21.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^5.0.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint"
    ]
  }
}
